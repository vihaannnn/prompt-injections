import pandas as pd

# File paths
file1 = 'extraction_robustness_dataset_first100.csv'
file2 = 'hijacking_robustness_dataset_first100.csv'

# Read the CSV files
df1 = pd.read_csv(file1)
df2 = pd.read_csv(file2)

# Print first 5 rows of first file
print("=" * 80)
print(f"First 5 rows of {file1}:")
print("=" * 80)
print(df1.head())
print(f"\nShape: {df1.shape}")

print("\n" + "=" * 80)
print(f"First 5 rows of {file2}:")
print("=" * 80)
print(df2.head())
print(f"\nShape: {df2.shape}")